<?xml version="1.0" encoding="UTF-8"?>
<div type="attributeDocumentation" xml:id="attr-target" xmlns:eac="http://archivists.org/ns/eac/v2" xmlns="http://www.tei-c.org/ns/1.0" xmlns:ead="https://archivists.org/ns/ead/v4">
               <head>
                  <att>target</att>
               </head>
               <div type="fullName">
                  <p>Target</p>
               </div>
               <div type="summary">
                  <p>A pointer to the ID of another element. </p>
               </div>
               <div type="description">
                     <p>Used to create internal links within an
                     XML instance. Optionally available in all elements except <gi>eac</gi>.</p>
               </div>
               <div type="datatype">
                  <p>IDREFS</p>
               </div>
               <div type="examples">
                  <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:lang="eng">
                     <eac:occupations>
                        <eac:occupation>
                           <eac:term>Assistant examiner - level III</eac:term>
                           <eac:placeName target="#place1">Bern</eac:placeName>
                        </eac:occupation>
                     </eac:occupations>
                     <eac:places>
                        <eac:place id="place1">
                           <eac:placeName>Bern</eac:placeName>
                           <eac:placeRole>Place of work</eac:placeRole>
                        </eac:place>
                     </eac:places>
                  </egXML>
               </div>
            </div>