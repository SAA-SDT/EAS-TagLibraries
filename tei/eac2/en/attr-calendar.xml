<?xml version="1.0" encoding="UTF-8"?>
<div type="attributeDocumentation" xml:id="attr-calendar" xmlns:eac="http://archivists.org/ns/eac/v2" xmlns="http://www.tei-c.org/ns/1.0">
               <head>
                  <att>calendar</att>
               </head>
               <div type="fullName">
                  <p>Calendar</p>
               </div>
               <div type="summary">
                  <p>System of reckoning time, such as Gregorian calendar or Julian calendar.
                     </p>
               </div>
               <div type="description">
                  <p>Suggested values include, but are not limited to, "gregorian" and "julian".
                     Available in <gi>date</gi>, <gi>fromDate</gi>, and <gi>toDate</gi>.
                  </p>
               </div>
               <div type="datatype">
                  <p>NMTOKEN</p>
               </div>
               <div type="examples">
                  <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:lang="eng">
                     <eac:dateRange>
                        <eac:fromDate calendar="gregorian" certainty="approximate" era="ce" standardDate="1950">1950</eac:fromDate>
                        <eac:toDate calendar="gregorian" certainty="approximate" era="ce" standardDate="2000">2000</eac:toDate>
                     </eac:dateRange>
                  </egXML>
               </div>
            </div>