<?xml version="1.0" encoding="UTF-8"?>
<div type="attributeDocumentation" xml:id="attr-certainty" xmlns:ead="http://ead3.archivists.org/schema/" xmlns="http://www.tei-c.org/ns/1.0">
               <head>
                  <att>certainty</att>
               </head>
               <div type="fullName">
                  <p>Certainty</p>
               </div>
               <div type="summary">
                  <p>The level of confidence for the information given in <gi>date</gi>, <gi>unitdate</gi>, or <gi>unitdatestructured</gi>, e.g., approximate or circa. </p>
               </div>
               <div type="datatype">
                  <p>NMTOKEN</p>
               </div>
               <div type="examples">
                  <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:lang="eng" valid="true">
                     <ead:unitdatestructured calendar="gregorian" certainty="approximate" datechar="creation" era="ce" unitdatetype="inclusive">
                        <ead:daterange>
                          <ead:fromdate standarddate="1950">1950</ead:fromdate>
                          <ead:todate standarddate="2000">2000</ead:todate>
                        </ead:daterange>
                     </ead:unitdatestructured>
                  </egXML>
               </div>

</div>
