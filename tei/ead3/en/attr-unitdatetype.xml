<?xml version="1.0" encoding="UTF-8"?>
<div type="attributeDocumentation" xml:id="attr-unitdatetype" xmlns:ead="http://ead3.archivists.org/schema/" xmlns="http://www.tei-c.org/ns/1.0">
               <head>
                  <att>unitdatetype</att>
               </head>
               <div type="fullName">
                  <p>Unit Date Type</p>
               </div>
               <div type="summary">
                  <p>Identifies the type of date expressed in <gi>unitdate</gi> or <gi>unitdatestructured</gi>.</p>
               </div>
               <div type="values">
                  <p>bulk, inclusive</p>
               </div>
               <div type="examples">
                  <egXML xmlns="http://www.tei-c.org/ns/Examples" xml:lang="eng" valid="true">
                     <ead:unitdatestructured certainty="circa" unitdatetype="inclusive">
                        <ead:daterange>
                           <ead:fromdate notbefore="1971" notafter="1975">around 1973</ead:fromdate>
                           <ead:todate standarddate="1992">1992</ead:todate>
                        </ead:daterange>
                     </ead:unitdatestructured>
                  </egXML>
               </div>
            </div>